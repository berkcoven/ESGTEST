@model ESG.Model.Entities.User
@{
    ViewBag.Title = "UserDetail";
}

<!-- BANNER WRAP -->
<div class="banner-wrap e-sport">
    <!-- BANNER -->
    <div class="banner grid-limit">
        <h2 class="banner-title">@Model.NickName</h2>
        <p class="banner-sections">
            <span class="banner-section">eSports</span>
            <!-- ARROW ICON -->
            <svg class="arrow-icon">
                <use xlink:href="#svg-arrow"></use>
            </svg>
            <!-- /ARROW ICON -->
            <span class="banner-section">Teams</span>
            <!-- ARROW ICON -->
            <svg class="arrow-icon">
                <use xlink:href="#svg-arrow"></use>
            </svg>
            <!-- /ARROW ICON -->
            <span class="banner-section">The Lone Wolves</span>
            <!-- ARROW ICON -->
            <svg class="arrow-icon">
                <use xlink:href="#svg-arrow"></use>
            </svg>
            <!-- /ARROW ICON -->
            <span class="banner-section">@Model.UserTeam.TeamName</span>
        </p>
    </div>
    <!-- /BANNER -->
</div>
<!-- /BANNER WRAP -->
<!-- LIVE NEWS WIDGET WRAP -->
